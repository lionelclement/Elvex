/************************************************************
* Patterns
************************************************************/
@m = [gender:ms];
@f = [gender:fm];
@_1 = [person:one];
@_2 = [person:two];
@_12 = [person:one|two];
@_13 = [person:one|three];
@_3 = [person:three];
@s = [number:sg];
@p = [number:pl];
@_1s = [@_1, @s];
@_2s = [@_2, @s];
@_12s = [@_12, @s];
@_13s = [@_13, @s];
@_3s = [@_3, @s];
@_1p = [@_1, @p];
@_2p = [@_2, @p];
@_3p = [@_3, @p];
@_3ms = [@_3s, @m];
@_3fs = [@_3s, @f];
@hum = [human:true];
@nohum = [human:false];
@def = [def:true];

/************************************************************
*  Forms
************************************************************/
FORM form;

/************************************************************
*  Variable words
************************************************************/
"[attrMorpho:$attrMorpho,attrPattern:$attrPattern,attrInteger:$attrInteger,attrString:$attrString]"	POS_2 [attrMorpho:$attrMorpho, attrPattern:$attrPattern,attrInteger:$attrInteger,attrString:$attrString];

