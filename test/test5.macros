// @macro	features
@s=	[number:singular];
@p=	[number:plural];
@m=	[gender:masc];
@f=	[gender:fem];
@ms= [@m, @s];
@mp= [@m, @p];
@fs= [@f, @s];
@fp= [@f, @p];
@_1=	[person:one];
@_2=	[person:two];
@_12=	[person:one|two];
@_3=	[person:three];
@_1s=	[@_1, @s];
@_2s=	[@_2, @s];
@_12s=	[@_12, @s];
@_3s=	[@_3, @s];
@def=	[def:true];


